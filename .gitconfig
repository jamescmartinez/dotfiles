[user]
	name = James Martinez
	email = jamescmartinez@gmail.com
[alias]
	amend = commit --amend -C HEAD
	undo = reset --soft HEAD^
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	s = status
[color]
	ui = 1
[difftool]
	prompt = false
[push]
	default = simple
[merge]
	tool = kdiff3
[core]
	excludesfile = ~/.gitignore
[help]
	autocorrect = 50
[rerere]
	enabled = 1
