[user]
	name = James Martinez
	email = jamescmartinez@gmail.com
[alias]
	co = checkout
	c = commit
	s = status
	b = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[color]
	ui = true
[difftool]
	prompt = false
[push]
	default = simple
[merge]
	tool = kdiff3
[core]
	excludesfile = ~/.gitignore
